<generatorConfiguration>
    &lt;!&ndash;数据库驱动jar
    <classPathEntry location="E:\mysql-connector-java-5.1.7-bin.jar" />
    &ndash;&gt;

    <context id="DB2Tables" targetRuntime="MyBatis3">
        &lt;!&ndash;去除注释  &ndash;&gt;
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        &lt;!&ndash; 数据库连接 &ndash;&gt;
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/mysql?serverTimezone=UTC"
                        userId="houcefei" password="123456" />


        &lt;!&ndash;默认false
           Java type resolver will always use java.math.BigDecimal if the database column is of type DECIMAL or NUMERIC.
         &ndash;&gt;
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        &lt;!&ndash;生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建  使用Maven生成在target目录下，会自动创建） &ndash;&gt;
        <javaModelGenerator targetPackage="com.zc.provider.pojo" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        &lt;!&ndash;生成SQLMAP文件 &ndash;&gt;
        <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        &lt;!&ndash;生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现  context id="DB2Tables" 修改targetRuntime="MyBatis3"  &ndash;&gt;
        <javaClientGenerator  targetPackage="com.zc.provider.dao"  targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

       &lt;!&ndash; &lt;!&ndash;对应数据库表 mysql可以加入主键自增 字段命名 忽略某字段等&ndash;&gt;
